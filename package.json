{
  "name": "@smartive-private/graphql-magic",
  "version": "1.0.0",
  "description": "",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^18"
  },
  "scripts": {
    "bootstrap": "npm ci && npm run generate",
    "generate": "npm run generate:index-files",
    "generate:index-files": "cti create ./src --excludes types --withoutbackup",
    "lint": "prettier --check src && eslint src",
    "lint:fix": "prettier --write src && eslint src --fix",
    "test": "npm run lint && npm run build",
    "clean": "del-cli dist",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.json",
    "publish": "semantic-release"
  },
  "overrides": {
    "graphql": "$graphql"
  },
  "browserslist": "> 0.25%, not dead",
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@types/node": "^18.15.11",
    "@apollo/server": "^4.0.0",
    "code-block-writer": "^12.0.0",
    "graphql": "^15.8.0",
    "inflection": "^2.0.1",
    "knex": "^2.4.2",
    "knex-schema-inspector": "^3.0.1",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-typescript": "7.21.4",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/gitlab": "12.0.1",
    "@semantic-release/npm": "10.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@smartive/eslint-config": "3.1.1",
    "@smartive/prettier-config": "3.0.0",
    "@types/lodash": "^4.14.194",
    "@types/luxon": "^3.3.0",
    "@types/uuid": "^9.0.1",
    "create-ts-index": "1.14.0",
    "del-cli": "5.0.0",
    "eslint": "8.38.0",
    "prettier": "2.8.7",
    "semantic-release": "21.0.1",
    "typescript": "5.0.4"
  }
}
